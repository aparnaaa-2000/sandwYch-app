{
  "name": "sandwychprototype",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "android": "npx react-native run-android",
    "ios": "npx react-native run-ios --simulator=\"iPhone 14 Pro\"",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "clean": "rm -rf ./ios/DerivedData && xcodebuild -workspace ./ios/sandwychprototype.xcworkspace -scheme sandwychprototype clean",
    "build": "xcodebuild build -workspace \"ios/sandwychprototype.xcworkspace\" -scheme \"sandwychprototype\" -sdk \"iphonesimulator\" -configuration \"Release\" -derivedDataPath \"./ios/DerivedData\"",
    "build-test": "npm run setup-env && npm run build && npm run automated-tests",
    "clean-build-test": "npm run clean && npm run setup-env && npm run build && npm run automated-tests",
    "clean-react-native": "watchman watch-del-all && rm -rf node_modules && npm install && rm -rf /tmp/metro-bundler-cache-* && npm start -- --reset-cache && rm -rf /tmp/haste-map-react-native-packager-*",
    "clean-packages": "rm -rf node_modules && rm -rf yarn.lock && rm -rf package-lock.json && rm -rf ./ios/Podfile.lock",
    "clean-install-ios": "yarn clean-packages && yarn install && cd ios && pod install && cd ..",
    "clean-install": "yarn clean-packages && yarn install && cd ios && pod install && cd .."
  },
